      "source": [
        "def calculate_total(price, tax_rate):\n",
        "    total = price + (price * tax_rate / 100)\n",
        "    return total\n",
        "\n",
        "# 関数を呼び出して戻り値の変数productを代入する\n",
        "product = calculate_total(110, 10)\n",
        "\n",
        "# 結果を出力する\n",
        "print(f\"税込金額は{product}円です。\")"
      ]
